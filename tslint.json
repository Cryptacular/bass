{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "linterOptions": {
        "exclude": [
            "src/polyfills/**/*"
        ]
    },
    "rules": {
        "object-literal-sort-keys": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case"
        ]
    },
    "rulesDirectory": []
}